<form-validation>
	<formset>
		<form name="nanoparticleSampleForm">
			<field
				property="particleSampleBean.domainParticleSample.name" page="1"
				depends="required">
				<arg position="0"
					key="nanoparticleSampleForm.sampleName" bundle="particle" />
			</field>
			<field
				property="particleSampleBean.pocBean.domain.id"
				page="1" depends="validwhen">
				<arg position="0"
					key="nanoparticleSampleForm.pointOfContact" bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((*this* != null) and (*this* > 0))
					</var-value>
				</var>
			</field>
		</form>
		<form name="chemicalAssociationForm">
			<field property="assoc.type" page="2" depends="required">
				<arg position="0" key="chemicalAssociationType"
					bundle="particle" />
			</field>
			<field property="assoc.attachment.bondType" page="2"
				depends="validwhen">
				<arg position="0" key="chemicalAssociationBondType"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((assoc.type!='attachment') or (*this*!=null))
					</var-value>
				</var>
			</field>
			<field property="assoc.associatedElementA.entityId" page="2"
				depends="required">
				<arg position="0" key="associatedElementA"
					bundle="particle" />
			</field>
			<field property="assoc.associatedElementB.entityId" page="2"
				depends="required">
				<arg position="0" key="associatedElementB"
					bundle="particle" />
			</field>
			<field
				property="assoc.associatedElementA.composingElement.id" page="2"
				depends="validwhen">
				<arg position="0"
					key="associatedElementAComposingElement" bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((assoc.associatedElementA.compositionType!='Nanoparticle
						Entity') or (*this*!=0))
					</var-value>
				</var>
			</field>
			<field
				property="assoc.associatedElementB.composingElement.id" page="2"
				depends="validwhen">
				<arg position="0"
					key="associatedElementBComposingElement" bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((assoc.associatedElementA.compositionType!='Nanoparticle
						Entity') or (*this*!=0))
					</var-value>
				</var>
			</field>
		</form>
		<form name="nanoparticleEntityForm">
			<field property="entity.type" page="2" depends="required">
				<arg position="0" key="nanoparticleEntityType"
					bundle="particle" />
			</field>
			<field property="entity.carbonNanotube.diameter" page="2"
				depends="float">
				<arg position="0"
					key="nanoparticleEntityForm.carbonNanotube.diameter"
					bundle="particle" />
			</field>
			<field property="entity.carbonNanotube.averageLength"
				page="2" depends="float">
				<arg position="0"
					key="nanoparticleEntityForm.carbonNanotube.averageLength"
					bundle="particle" />
			</field>
			<field property="entity.fullerene.numberOfCarbon" page="2"
				depends="integer">
				<arg position="0"
					key="nanoparticleEntityForm.fullerene.numberOfCarbon"
					bundle="particle" />
			</field>
			<field property="entity.biopolymer.name" page="2"
				depends="validwhen">
				<arg position="0"
					key="nanoparticleEntityForm.biopolymer.name" bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((entity.type!='biopolymer') or (*this*!=null))
					</var-value>
				</var>
			</field>
			<field property="entity.biopolymer.type" page="2"
				depends="validwhen">
				<arg position="0"
					key="nanoparticleEntityForm.biopolymer.type" bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((entity.type!='biopolymer') or (*this*!=null))
					</var-value>
				</var>
			</field>
			<field property="entity.liposome.polymerName" page="2"
				depends="validwhen">
				<arg position="0"
					key="nanoparticleEntityForm.liposome.polymerName"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((entity.type!='liposome') or
						((entity.liposome.polymerized=='false') or
						(*this*!=null)))
					</var-value>
				</var>
			</field>
			<field property="entity.emulsion.polymerName" page="2"
				depends="validwhen">
				<arg position="0"
					key="nanoparticleEntityForm.emulsion.polymerName"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((entity.type!='emulsion') or
						((entity.emulsion.polymerized=='false') or
						(*this*!=null)))
					</var-value>
				</var>
			</field>
			<field property="entity.polymer.crossLinkDegree" page="2"
				depends="validwhen">
				<arg position="0"
					key="nanoparticleEntityForm.polymer.crossLinkDegree"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((entity.type!='polymer') or
						((entity.polymer.crossLinkDegree=="No") or
						(*this*!=null)))
					</var-value>
				</var>
			</field>
			<field property="domainComposingElement.type" page="2"
				depends="validwhen"
				indexedListProperty="entity.composingElements">
				<arg position="0"
					key="nanoparticleEntityForm.elementType" bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>(*this*!=null)</var-value>
				</var>
			</field>
			<field property="domainComposingElement.name" page="2"
				depends="validwhen"
				indexedListProperty="entity.composingElements">
				<arg position="0"
					key="nanoparticleEntityForm.elementName" bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>(*this*!=null)</var-value>
				</var>
			</field>
		</form>
		<form name="functionalizingEntityForm">
			<field property="entity.type" page="2" depends="required">
				<arg position="0" key="functionalizingEntityType"
					bundle="particle" />
			</field>
			<field property="entity.name" page="2" depends="required">
				<arg position="0" key="functionalizingEntityName"
					bundle="particle" />
			</field>
			<field property="entity.value" page="2" depends="float">
				<arg position="0" key="functionalizingEntityForm.value"
					bundle="particle" />
			</field>
			<field property="type" page="2" depends="validwhen"
				indexedListProperty="entity.functions">
				<arg position="0"
					key="functionalizingEntity.functionType" bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>(*this*!=null)</var-value>
				</var>
			</field>
		</form>
		<form name="compositionFileForm">
			<field property="compFile.externalUrl" page="2"
				depends="validwhen">
				<arg position="0" key="externalUrl" bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						(((*this*!=null) and
						  (compFile.domainFile.uriExternal=="true")) or
						 (compFile.domainFile.uriExternal=="false"))
					</var-value>
				</var>
			</field>
			<field property="compFile.uploadedFile" page="2"
				depends="validwhen">
				<arg position="0" key="uploadedFile" bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						(((compFile.domainFile.uri!=null) or
						((*this*!=null) and
						(compFile.domainFile.uriExternal=="false"))) or
						(compFile.domainFile.uriExternal=="true"))
					</var-value>
				</var>
			</field>
			<field property="compFile.domainFile.title" page="2"
				depends="required,mask">
				<arg position="0" key="file.title" bundle="particle" />
				<msg name="mask" key="file.title.invalid" />
				<var>
					<var-name>mask</var-name>
					<var-value>^[a-zA-Z0-9\-\_\s]*$</var-value>
				</var>
			</field>
			<field property="compFile.domainFile.type" page="2"
				depends="required">
				<arg position="0" key="file.type" bundle="particle" />
			</field>
		</form>
