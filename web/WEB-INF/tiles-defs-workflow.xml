<tiles-definitions>
	<!-- definition for the manage assay page -->
	<definition name="workflow.manageproject" extends="calab.home">
		<put name="calabContent" value="/bodyManageItem.jsp" />
		<put name="menu" value="workflow" />
		<put name="item" value="Project" />
		<put name="itemDescription" value="/workflow/bodyProjectDescription.jsp"/>
		<put name="createLink" value="initSession.do?forwardPage=createProject" />
		<put name="searchLink" value="initSession.do?forwardPage=searchProject" />
	</definition>
	
	<!-- main layout definition for the workflow content page inside caLAB -->
	<definition name="workflow.content.home" path="/layout/workflowLayout.jsp">
		<put name="workflowActionMenu" value="/workflow/actionMenu.jsp" />
		<putList name="inActions">
			<item value="Use Aliquot" link="initSession.do?forwardPage=useAliquot" />
			<item value="Upload Files" link="initSession.do?forwardPage=fileUpload" />
			<item value="Download Files" link="initSession.do?forwardPage=fileDownload" />
			<item value="Mask Files" link="initSession.do?forwardPage=runFileMask" />
		</putList>
		<putList name="outActions">
			<item value="Upload Files" link="initSession.do?forwardPage=fileUpload" />
			<item value="Download Files" link="initSession.do?forwardPage=fileDownload" />
			<item value="Mask Files" link="initSession.do?forwardPage=runFileMask" />
		</putList>
		<putList name="uploadActions">
			<item value="Upload Input File" link="initSession.do?forwardPage=fileUpload&amp;inout=Input" />
			<item value="Upload Output File" link="initSession.do?forwardPage=fileUpload&amp;inout=Output" />
		</putList>
	</definition>

