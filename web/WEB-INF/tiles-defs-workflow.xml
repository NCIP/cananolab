<tiles-definitions>
	<!-- definition for the manage assay page -->
	<definition name="workflow.manageproject" extends="calab.home">
		<put name="calabContent" value="/bodyManageItem.jsp" />
		<put name="menu" value="workflow" />
		<put name="item" value="Project" />
		<put name="itemDescription" value="/workflow/bodyProjectDescription.jsp" />
		<put name="createLink" value="createProject.do" />
		<put name="searchLink" value="searchProject.do" />
	</definition>

	<!-- definition for the execute workflow page -->
	<definition name="workflow.executeworkflow" extends="calab.home">
		<put name="calabContent" value="/bodyManageItem.jsp" />
		<put name="menu" value="workflow" />
		<put name="item" value="Assay Run" />
		<put name="itemDescription" value="/workflow/bodyExecuteWorkflowDescription.jsp" />
		<put name="createLink" value="createRun.do?dispatch=setup" />
		<put name="searchLink" value="searchRun.do?dispatch=setup" />
	</definition>

	<!-- main layout definition for the workflow content page inside caNanoLab -->
	<definition name="workflow.content.home" path="/layout/workflowLayout.jsp">
		<put name="workflowActionMenu" value="/workflow/actionMenu.jsp?helpFlag=on" />
		<putList name="inActions">
			<item value="Use Aliquot" link="useAliquot.do?dispatch=setup" />
			<item value="Upload Files" link="fileUpload.do?dispatch=setup" />
			<item value="Download Files" link="runFile.do?dispatch=setupDownload" />
			<item value="Mask Files" link="runFile.do?dispatch=setupMask" />
		</putList>
		<putList name="outActions">
			<item value="Upload Files" link="fileUpload.do?dispatch=setup" />
			<item value="Download Files" link="runFile.do?dispatch=setupDownload" />
			<item value="Mask Files" link="runFile.do?dispatch=setupMask" />
		</putList>
		<putList name="runActions">
			<item value="Upload Input File" link="fileUpload.do?dispatch=setup&amp;inout=Input" />
			<item value="Upload Output File" link="fileUpload.do?dispatch=setup&amp;inout=Output" />
		</putList>
	</definition>

