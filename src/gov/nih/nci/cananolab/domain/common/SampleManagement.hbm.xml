<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.common" auto-import="false">
	<class name="SampleManagement" table="sample_management" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="sample_management_pk_id">
			<generator class="hilo">
                          <param name="table">hibernate_unique_key</param>
                          <param name="column">next_hi</param>
                        </generator>
		</id>
		<!-- Attributes mapping for the SampleManagement class -->
		<property name="comments" type="string" column="general_comments"/>
		<property name="createdBy" type="string" column="created_by"/>
		<property name="createdDate" type="java.util.Date" column="created_date"/>
		<property name="description" type="string" column="description"/>
		<property name="lotDescription" type="string" column="lot_description"/>
		<property name="lotId" type="string" column="lot_id"/>
		<property name="receivedBy" type="string" column="received_by"/>
		<property name="receivedDate" type="java.util.Date" column="received_date"/>
		<property name="sourceSampleId" type="string" column="source_sample_id"/>
		<!-- Associations mapping for the SampleManagement class -->
		<many-to-one name="nanoparticleSample" class="gov.nih.nci.cananolab.domain.particle.NanoparticleSample" column="particle_sample_pk_id" unique="true" lazy="proxy" />
		<set name="sampleContainerCollection" lazy="true">
			<cache usage="read-write" />
			<key column="sample_management_pk_id" />
			<one-to-many class="gov.nih.nci.cananolab.domain.common.SampleContainer"/>
		</set>
	</class>
</hibernate-mapping>
