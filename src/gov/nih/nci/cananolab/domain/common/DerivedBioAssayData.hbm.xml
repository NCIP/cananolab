<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.common"
	auto-import="false">
	<class name="DerivedBioAssayData" table="derived_bioassay_data"
		lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long"
			column="derived_bioassay_data_pk_id">
			<generator class="hilo">
				<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
			</generator>
		</id>
		<!-- Attributes mapping for the DerivedBioAssayData class -->
		<property name="createdBy" type="string" column="created_by" />
		<property name="createdDate" type="java.util.Date"
			column="created_date" />
		<!-- Associations mapping for the DerivedBioAssayData class -->
		<many-to-one name="labFile"
			class="gov.nih.nci.cananolab.domain.common.LabFile"
			column="file_pk_id" unique="true" lazy="proxy"
			cascade="save-update,delete" />
		<set name="derivedDatumCollection" lazy="false"
			cascade="all-delete-orphan">
			<cache usage="read-write" />
			<key column="derived_bioassay_data_pk_id" />
			<one-to-many
				class="gov.nih.nci.cananolab.domain.common.DerivedDatum" />
		</set>
	</class>
</hibernate-mapping>
