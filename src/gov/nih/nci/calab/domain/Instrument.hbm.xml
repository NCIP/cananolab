<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
	<class name="Instrument" table="INSTRUMENT" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long" column="INSTRUMENT_PK_ID" unsaved-value="0">
			<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
			</generator>
		</id>
		<property name="type" type="java.lang.String" column="TYPE" />
		<property name="manufacturer" type="java.lang.String" column="MANUFACTURER" />
		<property name="abbreviation" type="java.lang.String" column="ABBREVIATION" />
	</class>
</hibernate-mapping>