<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
  <class name="Run" table="Run" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="RUN_PK_ID" unsaved-value="0">
      <generator class="org.hibernate.id.TableHiLoGenerator">
  	  			<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
	   </generator>
    </id>
    <property name="name" type="java.lang.String" column="RUN_NAME" />
    <property name="description" type="java.lang.String" column="DESCRIPTION" />
    <property name="createdBy" type="java.lang.String" column="CREATED_BY" />
    <property name="createdDate" type="java.util.Date" column="CREATED_DATE" />
    <property name="runDate" type="java.util.Date" column="RUN_DATE" />
    <property name="runBy" type="java.lang.String" column="RUN_BY" />
    <many-to-one name="assay" class="gov.nih.nci.calab.domain.Assay" column="ASSAY_PK_ID"/>
    <set name="runSampleContainerCollection" cascade="all-delete-orphan" lazy="true" inverse="true">
      <cache usage="read-write" />
      <key column="RUN_PK_ID" />
      <one-to-many class="gov.nih.nci.calab.domain.RunSampleContainer" />
    </set>
    <set name="inputFileCollection" cascade="all-delete-orphan" lazy="true" inverse="true">
      <cache usage="read-write" />
      <key column="RUN_PK_ID" />
      <one-to-many class="gov.nih.nci.calab.domain.InputFile" />
    </set>
    <set name="outputFileCollection" cascade="all-delete-orphan" lazy="true" inverse="true">
      <cache usage="read-write" />
      <key column="RUN_PK_ID" />
      <one-to-many class="gov.nih.nci.calab.domain.OutputFile" />
    </set>
  </class>
</hibernate-mapping>

