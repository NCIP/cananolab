<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
	<class name="RunSampleContainer" table="Run_Sample_Container" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long" column="RUN_SAMPLE_CONTAINER_PK_ID" unsaved-value="0">
			<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
			</generator>
		</id>
		<property name="comments" type="java.lang.String" column="COMMENTS" />
		<property name="createdBy" type="java.lang.String" column="CREATED_BY" />
		<property name="createdDate" type="java.util.Date" column="CREATED_DATE" />
		<many-to-one name="sampleContainer" class="gov.nih.nci.calab.domain.SampleContainer" column="SAMPLE_CONTAINER_PK_ID" lazy="false" fetch="join" />
		<many-to-one name="run" class="gov.nih.nci.calab.domain.Run" column="RUN_PK_ID" cascade="all-delete-orphan" />
        <many-to-one name="dataStatus" class="gov.nih.nci.calab.domain.DataStatus" column="DATA_STATUS_PK_ID" />
	</class>
</hibernate-mapping>

