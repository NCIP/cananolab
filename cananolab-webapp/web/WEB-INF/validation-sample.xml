				<var>
					<var-name>test</var-name>
					<var-value>
						((dispatch!="addFile") or (
						((*this*!=null) and
						(achar.theFinding.theFile.domainFile.uriExternal=="true")) or
						(achar.theFinding.theFile.domainFile.uriExternal=="false")))
					</var-value>
				</var>
			</field>
