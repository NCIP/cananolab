<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
  <class name="Contact" table="Contact" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="CONTACT_PK_ID">
      <generator class="assigned" />
    </id>
    <property name="firstName" type="java.lang.String" column="FIRST_NAME" />
    <property name="lastName" type="java.lang.String" column="LAST_NAME" />
    <property name="title" type="java.lang.String" column="TITLE" />
    <property name="phoneNumber" type="java.lang.String" column="PHONE_NUMBER" />
    <property name="email" type="java.lang.String" column="EMAIL" />
    <property name="updateDate" type="java.util.Date" column="UPDATE_DATE" />
    <property name="middleName" type="java.lang.String" column="MIDDLE_NAME" />
    <property name="fax" type="java.lang.String" column="FAX" />
    <property name="address" type="java.lang.String" column="ADDRESS" />
    <property name="city" type="java.lang.String" column="CITY" />
    <property name="state" type="java.lang.String" column="STATE" />
    <property name="country" type="java.lang.String" column="COUNTRY" />
    <property name="postalCode" type="java.lang.String" column="POSTAL_CODE" />
    <property name="piName" type="java.lang.String" column="PI_NAME" />
    <set name="sourceCollection" table="Source_contact" lazy="true">
      <cache usage="read-write" />
      <key column="CONTACT_PK_ID" />
      <many-to-many class="gov.nih.nci.calab.domain.Source" column="SOURCE_PK_ID" />
    </set>
  </class>
</hibernate-mapping>

