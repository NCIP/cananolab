<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.common" auto-import="false">
	<class name="ChemicalAssociation" table="chemical_association" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="chemical_association_pk_id">
			<generator class="hilo">
                          <param name="table">hibernate_unique_key</param>
                          <param name="column">next_hi</param>
                        </generator>
		</id>
		<discriminator column="discriminator" type="string"/>
		<!-- Attributes mapping for the ChemicalAssociation class -->
		<property name="createdBy" type="string" column="created_by"/>
		<property name="createdDate" type="java.util.Date" column="created_date"/>
		<property name="description" type="string" column="description"/>
		<!-- Associations mapping for the ChemicalAssociation class -->
		<set name="fileCollection" table="chemical_association_file" lazy="true" inverse="false">
			<cache usage="read-write" />
			<key column="chemical_association_pk_id" />
			<many-to-many class="gov.nih.nci.cananolab.domain.common.File" column="file_pk_id" />
		</set>
		<many-to-one name="sampleComposition" class="gov.nih.nci.cananolab.domain.common.SampleComposition" column="composition_pk_id" lazy="proxy" />
		<many-to-one name="associatedElementA" class="gov.nih.nci.cananolab.domain.common.AssociatedElement" column="associated_element_a_pk_id" lazy="proxy" />
		<many-to-one name="associatedElementB" class="gov.nih.nci.cananolab.domain.common.AssociatedElement" column="associated_element_b_pk_id" lazy="proxy" />
		<subclass name="gov.nih.nci.cananolab.domain.material.OtherChemicalAssociation" discriminator-value="OtherChemicalAssociation">
			<!-- Attributes mapping for the OtherChemicalAssociation class -->
			<property name="type" type="string" column="other_chemical_association_type"/>

		</subclass>
		<subclass name="gov.nih.nci.cananolab.domain.material.Attachment" discriminator-value="Attachment">
			<!-- Attributes mapping for the Attachment class -->
			<property name="bondType" type="string" column="attachment_bond_type"/>

		</subclass>
		<subclass name="gov.nih.nci.cananolab.domain.material.Entrapment" discriminator-value="Entrapment">

		</subclass>
		<subclass name="gov.nih.nci.cananolab.domain.material.Encapsulation" discriminator-value="Encapsulation">

		</subclass>
	</class>
</hibernate-mapping>
