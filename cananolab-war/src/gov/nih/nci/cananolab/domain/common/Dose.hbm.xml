<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.common" auto-import="false">
	<class name="Dose" table="dose" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="dose_pk_id">
			<generator class="hilo">
                          <param name="table">hibernate_unique_key</param>
                          <param name="column">next_hi</param>
                        </generator>
		</id>
		<!-- Attributes mapping for the Dose class -->
		<property name="value" type="float" column="value"/>
		<property name="valueUnit" type="string" column="value_unit"/>
		<property name="volume" type="float" column="volume"/>
		<property name="volumeUnit" type="string" column="volume_unit"/>
		<!-- Associations mapping for the Dose class -->
	</class>
</hibernate-mapping>
